<!DOCTYPE html>
<html>
<head>
    <title>Cars</title>
</head>
<body>
    <%- include('../partials/_navbar.ejs') %>
    <h1>Cars</h1>
    <ul>
        <% cars.forEach(car => { %>
            <li>
                <a href="/cars/<%= car._id %>"><%= car.name %></a>
            </li>
        <% }) %>
    </ul>
    <a href="/cars/new">Add a new car</a>
</body>
</html>
